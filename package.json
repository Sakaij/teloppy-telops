        "aws-sdk": "^2.1035.0",
        "axios": "^0.24.0",
        "gsap": "^3.10.4",
        "jquery": "^3.6.0",
        "jquery.ripples": "^0.6.3",
        "mime-type": "^4.0.0",
        "three": "^0.141.0",
        "typefaces": "^0.0.1"
    },
    "devDependencies": {
        "@types/jest": "^27.0.3",
        "child_process": "^1.0.2",
        "commander": "^8.3.0",
        "copy-webpack-plugin": "^9.1.0",
        "core-js": "^3.19.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.2.0",
        "dotenv": "^10.0.0",
        "dotenv-webpack": "^7.0.3",
        "ejs": "^3.1.6",
        "ejs-compiled-loader": "^3.1.0",
        "file-loader": "^6.2.0",
        "flush-promises": "^1.0.2",
        "glob": "^7.2.0",
        "html-loader": "^3.0.1",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^27.4.5",
        "jest-css-modules": "^2.1.0",
        "mini-css-extract-plugin": "^2.4.5",
        "rimraf": "^3.0.2",
        "sass": "^1.43.4",
        "sass-loader": "^12.3.0",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.3.3",
        "ts-jest": "^27.1.2",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.2",
        "url-loader": "^4.1.1",
        "webpack": "^5.64.1",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.5.0"
    },
    "scripts": {
        "devcleanup": "rimraf ./.dev",
        "distcleanup": "rimraf ./dist",
        "dev": "cross-env npm run devcleanup && cross-env NODE_ENV=development webpack-dev-server --hot --mode development --config webpack.dev.js",
        "build-staging-noclean": "cross-env NODE_ENV=staging webpack --mode production --config webpack.build.js",
        "build-production-noclean": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.js",
        "build-staging": "npm run distcleanup && cross-env NODE_ENV=staging webpack --mode production --config webpack.build.js",
        "build-production": "npm run distcleanup && cross-env NODE_ENV=production webpack --mode production --config webpack.build.js",
        "deploy-staging": "npm run distcleanup && cross-env NODE_ENV=staging webpack --mode production --config webpack.build.js",
        "deploy-production": "npm run distcleanup && cross-env NODE_ENV=production webpack --mode production --config webpack.build.js",
        "deploy-staging-all": "npm run distcleanup && node deploy/buildAll --env staging && node deploy/distDeploy --env staging",
        "deploy-production-all": "npm run distcleanup && node deploy/buildAll --env production && node deploy/distDeploy --env production",
        "postdeploy-staging": "node deploy/distDeploy --env staging",
        "postdeploy-production": "node deploy/distDeploy --env production",
        "test": "jest",
        "test-watch": "jest --watch"
    },
    "license": "ISC"
}
