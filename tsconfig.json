{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"lib": [
			"esnext",
			"dom",
			"webworker"
		],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": "./",
		"resolveJsonModule": true,
		"paths": {
			"~/*": [
				"./*"
			],
			"@/*": [
				"./*"
			]
		}
	}
}